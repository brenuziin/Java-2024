package com.mycompany.atividadedois;

/**
 *
 * @author Aluno
 */
public class Livros {
    
    String titulo;
    String autor;
    int paginas;
    float preco;

    public Livros(String titulo, String autor, int paginas, float preco) {
        this.titulo = titulo;
        this.autor = autor;
        this.paginas = paginas;
        this.preco = preco;
    }
    
}
